<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="cl.pingon">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".LoginActivity"
            android:label="@string/title_activity_login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"/>
        <activity android:name=".BuzonActivity" />
        <activity
            android:name=".NuevoFormularioActivity"
            android:label="@string/title_activity_nuevo_formulario"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".InformesActivity" />
        <activity
            android:name=".InformeDetallesActivity"
            android:label="@string/title_activity_informe_detalles"
            android:theme="@style/AppTheme.NoActionBar"/>
    </application>

</manifest>